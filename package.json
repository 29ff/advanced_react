{
  "name": "advanced_react",
  "version": "1.0.0",
  "description": "Advanced React",
  "main": "lib/server.js",
  "repository": "git@github.com:29ff/advanced_react.git",
  "author": "29FF <nbhung1809@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "NODE_PATH=./lib pm2 --interpreter babel-node start ./lib/server.js --watch && pm2 logs",
    "stop": "pm2 stop all",
    "reload": "pm2 reload all && pm2 logs",
    "build:dev": "rm -rf ./public/*.js && rm -rf ./public/*css && webpack --progress --colors --watch --config webpack.config.dev.js",
    "build:prod": "rm -rf ./public/*.js && rm -rf ./public/*css && webpack --progress --colors --config webpack.config.prod.js",
    "test": "NODE_PATH=./lib jest",
    "test:clean": "rimraf ./coverage",
    "coverage": "yarn test:clean && NODE_PATH=./lib jest --coverage"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/lib/__mocks__/fileMock.js",
      "\\.(css|less|styl|scss|sass)$": "<rootDir>/lib/__mocks__/styleMock.js"
    },
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^22.0.4",
    "node-sass": "^4.7.2",
    "pm2": "^2.9.1",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "global": "^4.3.2",
    "lodash.debounce": "^4.0.8",
    "lodash.pickby": "^4.6.0",
    "moment": "^2.20.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "webpack": "^3.10.0"
  }
}
